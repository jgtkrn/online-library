{
  "name": "test",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "npm run copy:template",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nodemon --config nodemon.json",
    "start:debug": "NODE_ENV=debug  nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/src/main",
    "migration:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -n MigrationTable -d src/migrations",
    "migration:run": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run",
    "migration:revert": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "copy:template": "cp -r ./src/modules/users/templates/ ./dist/src/modules/users/templates && cp -r ./src/modules/app/i18n/ ./dist/src/modules/app/i18n  && cp -r ./src/modules/subscription/templates/ ./dist/src/modules/subscription/templates",
    "documentation:serve": "npx @compodoc/compodoc -p tsconfig.json --serve",
    "documentation:generate": "npx @compodoc/compodoc -p tsconfig.json"
  },
  "dependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/bull": "^0.6.1",
    "@nestjs/common": "^7.6.5",
    "@nestjs/core": "^7.0.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/schedule": "^0.4.2",
    "@nestjs/swagger": "^4.6.0",
    "@nestjs/typeorm": "^7.1.4",
    "@types/bcryptjs": "^2.4.2",
    "aws-sdk": "^2.770.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "coralogix-logger-winston": "^1.0.11",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "fb": "^2.0.0",
    "google-auth-library": "^7.0.4",
    "google-play-billing-validator": "^2.1.3",
    "googleapis": "^66.0.0",
    "in-app-purchase": "^1.11.4",
    "jose": "^2.0.3",
    "json2csv": "^5.0.6",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.0",
    "moment-timezone": "^0.5.38",
    "nest-winston": "^1.4.0",
    "nestjs-i18n": "^8.0.11",
    "node-apple-receipt-verify": "^1.9.3",
    "nodemailer": "^6.4.16",
    "passport": "^0.4.1",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "stripe": "^8.127.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.26",
    "uuid": "^8.3.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bull": "^3.15.9",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
